{
  "name": "tracsmobile",
  "version": "1.2.3",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle-ios": "react-native bundle --dev false --entry-file index.ios.js --bundle-output ios/main.jsbundle --platform ios",
    "bundle-ios-dev": "react-native bundle --dev true --entry-file index.ios.js --bundle-output ios/main.jsbundle --platform ios",
    "test": "jest --watch"
  },
  "standard": {
    "ignore": [
      "node_modules/**",
      "tests/**"
    ],
    "env": [
      "jest"
    ],
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@remobile/react-native-toast": "^1.0.7",
    "axios": "^0.18.0",
    "base-64": "^0.1.0",
    "dayjs": "^1.6.7",
    "form-data": "^2.3.2",
    "jest": "^23.2.0",
    "lodash.isequal": "^4.5.0",
    "mathjs": "^4.4.1",
    "moment": "^2.22.0",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-calendars": "^1.20.0",
    "react-native-cookies": "^3.2.0",
    "react-native-fcm": "^9.7.2",
    "react-native-firebase": "^3.2.4",
    "react-native-keychain": "^1.2.1",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-material-ripple": "^0.7.5",
    "react-native-modal": "^6.4.0",
    "react-native-popup-menu": "^0.13.2",
    "react-native-push-notification": "^3.0.1",
    "react-native-storage": "^0.2.2",
    "react-native-swipeout": "^2.3.1",
    "react-native-swiper": "^1.5.13",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.0.1",
    "react-navigation-fluid-transitions": "^0.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.3.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "csv-parse": "^2.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "lodash": "^4.17.10",
    "react-dom": "^16.1.1",
    "react-native-debugger-open": "^0.3.17",
    "react-test-renderer": "^16.1.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.3",
    "standard": "^11.0.1"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/tests/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests/mocks/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/dist/styled-components.native.cjs.js"
    },
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
