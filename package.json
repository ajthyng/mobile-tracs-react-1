{
  "name": "tracsmobile",
  "version": "1.2.3",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle-ios": "react-native bundle --dev false --entry-file index.ios.js --bundle-output ios/main.jsbundle --platform ios",
    "bundle-ios-dev": "react-native bundle --dev true --entry-file index.ios.js --bundle-output ios/main.jsbundle --platform ios",
    "ios-release": "react-native run-ios --configuration=\"Release\" --device=\"Robot 1-X iPhone 7\"",
    "android-release": "react-native run-android --variant=release",
    "test": "jest --watch"
  },
  "standard": {
    "ignore": [
      "node_modules/**",
      "tests/**"
    ],
    "env": [
      "jest",
      "browser"
    ],
    "parser": "babel-eslint"
  },
  "rnpm": {
    "assets": [
      "./fonts"
    ]
  },
  "dependencies": {
    "@remobile/react-native-toast": "^1.0.7",
    "axios": "^0.18.0",
    "base-64": "^0.1.0",
    "dayjs": "^1.7.5",
    "form-data": "^2.3.2",
    "jest": "^23.6.0",
    "lodash.isequal": "^4.5.0",
    "mathjs": "^5.1.2",
    "moment": "^2.22.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-native": "^0.57.0",
    "react-native-calendars": "^1.20.0",
    "react-native-cookies": "^3.3.0",
    "react-native-fabric": "^0.5.1",
    "react-native-fabric-crashlytics": "^0.1.8",
    "react-native-fcm": "^16.2.3",
    "react-native-firebase": "^4.3.8",
    "react-native-htmlview": "^0.13.0",
    "react-native-keychain": "^3.0.0",
    "react-native-linear-gradient": "^2.4.2",
    "react-native-material-ripple": "^0.8.0",
    "react-native-modal": "^6.5.0",
    "react-native-popup-menu": "^0.14.0",
    "react-native-push-notification": "^3.0.1",
    "react-native-shadow": "^1.2.2",
    "react-native-storage": "^0.2.2",
    "react-native-svg": "^6.5.2",
    "react-native-swipeout": "^2.3.6",
    "react-native-swiper": "^1.5.13",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.14.0",
    "react-navigation-fluid-transitions": "^0.2.6",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^3.4.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-preset-es2015": "6.24.1",
    "csv-parse": "^3.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "lodash": "^4.17.11",
    "metro-react-native-babel-preset": "^0.45.0",
    "react-dom": "^16.5.1",
    "react-native-debugger-open": "^0.3.17",
    "react-test-renderer": "^16.5.1",
    "redux-devtools-extension": "^2.13.2",
    "standard": "^12.0.1"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests/mocks/"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ],
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/dist/styled-components.native.cjs.js"
    },
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
