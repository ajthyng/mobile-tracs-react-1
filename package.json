{
  "name": "TRACSMobile",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle-ios": "react-native bundle --dev false --entry-file index.ios.js --bundle-output ios/main.jsbundle --platform ios",
    "postinstall": "rndebugger-open",
    "test": "jest --watch",
    "storybook": "storybook start -p 7007"
  },
  "dependencies": {
    "@remobile/react-native-toast": "^1.0.7",
