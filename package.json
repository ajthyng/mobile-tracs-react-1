    "jest": "^22.4.3",
    "mathjs": "^4.4.1",
    "moment": "^2.22.0",
    "prop-types": "^15.5.10",
    "react": "16.3.1",
    "react-native": "0.55.4",
    "react-native-calendars": "^1.19.3",
    "react-native-cookies": "^3.2.0",
    "react-native-dialogs": "^0.0.21",
    "react-native-fcm": "^9.7.2",
    "react-native-firebase": "^3.2.4",
    "react-native-foldview": "^1.1.1",
    "react-native-keychain": "^1.2.1",
    "react-native-material-ripple": "^0.7.5",
    "react-native-push-notification": "^3.0.1",
    "react-native-router-flux": "^4.0.0-beta.31",
    "react-native-sfsafariviewcontroller": "^0.2.4",
    "react-native-storage": "^0.2.2",
