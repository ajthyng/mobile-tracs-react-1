  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "rndebugger-open",
    "test": "jest --watch",
    "storybook": "storybook start -p 7007"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "bluebird": "^3.5.1",
    "moment": "^2.19.2",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-native": "^0.50.3",
    "react-native-cookies": "^3.2.0",
    "react-native-fcm": "^9.7.2",
    "react-native-keychain": "^1.2.1",
    "react-native-material-ripple": "^0.7.5",
    "react-native-push-notification": "^3.0.1",
    "react-native-router-flux": "^4.0.0-beta.24",
    "react-native-storage": "^0.2.2",
    "react-native-switch": "^1.2.1",
    "react-native-vector-icons": "^4.4.2",
    "react-native-wkwebview-reborn": "^1.10.0",
    "react-navigation": "^1.0.0-beta.19",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.15",
    "@storybook/addon-links": "^3.2.15",
    "@storybook/react-native": "^3.2.15",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-native": "^4.0.0",
    "jest": "^21.2.1",
    "react-dom": "^16.1.1",
    "react-native-debugger-open": "^0.3.15",
    "react-test-renderer": "^16.1.1"
  },
  "jest": {
    "preset": "react-native",
    "testMatch": [
      "**/tests/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests/mocks/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-navigation)/"
    ]
  }
}
